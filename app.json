{
  "expo": {
    "name": "MostBudget",
    "slug": "most-budget",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/MostBudgetLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/MostBudgetLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.most-budget"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/MostBudgetLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.most_budget"
    },
    "web": {
      "favicon": "./assets/images/MostBudgetLogo.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}